                <li class="wow bounceInUp c-review__item" data-wow-delay="0.2s">
                   <div class="c-review__content">
                        <figure class="c-review__img">
                            <img alt="xe ba gac van chuyen" src="/assets/img/sv1.png">
                        </figure>
                        <div class="c-review__star">
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
